{
    "homepage": "https://mixxx.org",
    "description": "Mixxx is Free DJ software that gives you everything you need to perform live mixes.",
    "version": "2.2.1",
    "bin": [
        [
            "Mixxx\\mixxx.exe",
            "mixxx",
            "--settingsPath \"$persist_dir\\appdata\""
        ]
    ],
    "shortcuts": [
        [
            "Mixxx\\mixxx.exe",
            "Mixxx",
            "--settingsPath \"$persist_dir\\appdata\""
        ]
    ],
    "persist": "appdata",
    "license": "GPL-2.0+",
    "dependencies": [
        "wixtoolset",
        "vcredist2015"
    ],
    "architecture": {
        "32bit": {
            "url": "https://downloads.mixxx.org/mixxx-2.2.1/mixxx-2.2.1-win32.exe#!/_setup.exe",
            "hash": "f7baab46840eb3e45b43e47f5685efd87fa4af6765f44708376c5cf4ae5ae4d9"
        },
        "64bit": {
            "url": "https://downloads.mixxx.org/mixxx-2.2.1/mixxx-2.2.1-win64.exe#!/_setup.exe",
            "hash": "65d4c02e27320a8a3ff8a22d1c4b4e09704cc88315384ba2d4482d93930afab9"
        }
    },
    "checkver": {
        "url": "https://mixxx.org/download/",
        "re": "\\bhttps://downloads.mixxx.org/mixxx-([\\d.]+)\\b"
    },
    "autoupdate": {
        "hash": {
            "url": "https://downloads.mixxx.org/mixxx-$version/mixxx-$version.sha256sum"
        },
        "architecture": {
            "32bit": {
                "url": "https://downloads.mixxx.org/mixxx-$version/mixxx-$version-win32.exe#!/_setup.exe"
            },
            "64bit": {
                "url": "https://downloads.mixxx.org/mixxx-$version/mixxx-$version-win64.exe#!/_setup.exe"
            }
        }
    },
    "_extra": {
        "mixxx.mst": ""
    },
    "pre_install": [
        "dark -nologo -sw -sct -sdet -sui -x \"$dir\\_1\" \"$dir\\_setup.exe\" NUL | Out-Null",
        "rm \"$dir\\_setup.exe\"",
        "[IO.File]::WriteAllBytes(\"$dir\\_transform.mst\", [Convert]::FromBase64String($manifest._extra.\"mixxx.mst\"))",
        "run 'msiexec' @('/a', \"`\"$((get-item \"$dir\\_1\\AttachedContainer\\*.msi\").FullName)`\"\", '/qn', \"TARGETDIR=`\"$dir`\"\", \"TRANSFORMS=`\"$dir\\_transform.mst`\"\")",
        "rm \"$dir\\*.*\"",
        "rm -Recurse \"$dir\\_1\""
    ]
}
